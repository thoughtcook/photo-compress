{
  "name": "photo-compress",
  "version": "1.0.0",
  "description": "A pure frontend image compression tool supporting JPEG, PNG, WebP, and AVIF formats",
  "main": "index.html",
  "scripts": {
    "serve": "python -m http.server 8000",
    "serve-node": "npx http-server -p 8000 -o",
    "dev": "npx live-server --port=8000"
  },
  "keywords": [
    "image-compression",
    "frontend",
    "javascript",
    "canvas",
    "webworker",
    "jpeg",
    "png",
    "webp",
    "avif"
  ],
  "author": "Photo Compress Tool",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "commonjs",
  "dependencies": {
    "playwright": "^1.55.1"
  }
}
